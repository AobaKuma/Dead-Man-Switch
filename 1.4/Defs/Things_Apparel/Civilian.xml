<?xml version="1.0" encoding="utf-8"?>
<Defs>

	<ThingDef ParentName="ApparelBase">
		<defName>DMS_Apparel_HazardSuit</defName>
		<label>hazard suit</label>
		<description>A large garment that conceals the wearer's entire head and body. worn as protection against hazardous materials.</description>
		<techLevel>Industrial</techLevel>
		<recipeMaker>
			<workSpeedStat>GeneralLaborSpeed</workSpeedStat>
			<workSkill>Crafting</workSkill>
			<effectWorking>Tailor</effectWorking>
			<soundWorking>Recipe_Tailor</soundWorking>
			<recipeUsers>
				<li>ElectricTailoringBench</li>
				<li>HandTailoringBench</li>
			</recipeUsers>
			<researchPrerequisite>DMS_Plasfiber</researchPrerequisite>
			<unfinishedThingDef>UnfinishedApparel</unfinishedThingDef>
		</recipeMaker>
		<costList>
			<Plasfiber>80</Plasfiber>
			<Steel>20</Steel>
			<Chemfuel>20</Chemfuel>
			<ComponentStamped>1</ComponentStamped>
		</costList>
		<tradeTags>
			<li>Clothing</li>
		</tradeTags>
		<graphicData>
			<texPath>Things/Apparel/HazardSuit/Apparel</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<thingCategories>
			<li>ApparelMisc</li>
		</thingCategories>
		<statBases>
			<MaxHitPoints>100</MaxHitPoints>
			<WorkToMake>5000</WorkToMake>
			<Mass>10</Mass>
			<ArmorRating_Sharp>0.1</ArmorRating_Sharp>
			<ArmorRating_Blunt>0.4</ArmorRating_Blunt>
			<ArmorRating_Heat>0.8</ArmorRating_Heat>
			<Insulation_Cold>20</Insulation_Cold>
			<Insulation_Heat>9</Insulation_Heat>
			<EquipDelay>1.5</EquipDelay>
		</statBases>
		<equippedStatOffsets>
			<MoveSpeed>-0.5</MoveSpeed>
			<ToxicEnvironmentResistance>1</ToxicEnvironmentResistance>
		</equippedStatOffsets>
		<apparel>
			<bodyPartGroups>
				<li>FullHead</li>
				<li>UpperHead</li>
				<li>Neck</li>
				<li>Shoulders</li>
				<li>Torso</li>
				<li>Legs</li>
				<li>Arms</li>
			</bodyPartGroups>
			<wornGraphicPath>Things/Apparel/HazardSuit/Apparel</wornGraphicPath>
			<shellCoversHead>true</shellCoversHead>
			<blocksAddedPartWoundGraphics>true</blocksAddedPartWoundGraphics>
			<immuneToToxGasExposure>true</immuneToToxGasExposure>
			<layers>
				<li>Shell</li>
			</layers>
			<defaultOutfitTags>
				<li>Worker</li>
			</defaultOutfitTags>
			<canBeGeneratedToSatisfyWarmth>false</canBeGeneratedToSatisfyWarmth>
			<canBeGeneratedToSatisfyToxicEnvironmentResistance>true</canBeGeneratedToSatisfyToxicEnvironmentResistance>
		</apparel>
	</ThingDef>

	<ThingDef ParentName="ApparelBase">
		<defName>DMS_Apparel_PowerSkelton</defName>
		<label>power skelton unit</label>
		<description>Physical enhancement equipment from array of neural prosthetic skeltons. It can increase the wearer's carry weight and mobility with effectively.</description>
		<techLevel>Industrial</techLevel>
		<recipeMaker>
			<workSpeedStat>WorkSpeedGlobal</workSpeedStat>
			<effectWorking>ConstructMetal</effectWorking>
			<soundWorking>Recipe_Machining</soundWorking>
			<researchPrerequisite>DMS_Prosthetic</researchPrerequisite>
			<recipeUsers>
			  <li>DMS_TableMachinePrinter</li>
			</recipeUsers>
			<unfinishedThingDef>UnfinishedApparel</unfinishedThingDef>
		</recipeMaker>
		<costList>
			<Tungsteel>75</Tungsteel>
			<Plasfiber>50</Plasfiber>
			<Uranium>20</Uranium>
			<ComponentStamped>7</ComponentStamped>
		</costList>
		<tradeTags>
			<li>Clothing</li>
		</tradeTags>
		<graphicData>
			<texPath>Things/Apparel/ExoSkelton/Apparel</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<thingCategories>
			<li>ApparelArmor</li>
		</thingCategories>
		<statBases>
			<MaxHitPoints>120</MaxHitPoints>
			<WorkToMake>8000</WorkToMake>
			<Mass>9</Mass>
			<ArmorRating_Sharp>0.75</ArmorRating_Sharp>
			<ArmorRating_Blunt>0.75</ArmorRating_Blunt>
			<ArmorRating_Heat>0.6</ArmorRating_Heat>
			<Insulation_Cold>0</Insulation_Cold>
			<Insulation_Heat>0</Insulation_Heat>
			<EquipDelay>30</EquipDelay>
		</statBases>
		<equippedStatOffsets>
			<MoveSpeed>-1</MoveSpeed>
		</equippedStatOffsets>
		<apparel>
			<bodyPartGroups>
				<li>Hands</li>
				<li>Feet</li>
				<li>Legs</li>
				<li>Arms</li>
			</bodyPartGroups>
			<wornGraphicPath>Things/Apparel/ExoSkelton/Apparel</wornGraphicPath>
			<blocksAddedPartWoundGraphics>true</blocksAddedPartWoundGraphics>
			<layers>
				<li>Shell</li>
			</layers>
			<defaultOutfitTags>
				<li>Soldier</li>
			</defaultOutfitTags>
			<canBeGeneratedToSatisfyWarmth>false</canBeGeneratedToSatisfyWarmth>
			<canBeGeneratedToSatisfyToxicEnvironmentResistance>false</canBeGeneratedToSatisfyToxicEnvironmentResistance>
		</apparel>
		<modExtensions>
			<li Class="VFECore.ApparelExtension">
				<carryingCapacity>100</carryingCapacity>
				<pawnCapacityMinLevels>
					<li>
						<capacity>Moving</capacity>
						<minLevel>0.5</minLevel>
					</li>
					<li>
						<capacity>Manipulation</capacity>
						<minLevel>0.5</minLevel>
					</li>
				</pawnCapacityMinLevels>
			</li>
		</modExtensions>
	</ThingDef>

</Defs>