        </tools>
      </li>
		<li Class="EBF.Hediffs.HediffCompProperties_MaxHPAdjust" MayRequire="V1024.EBFramework">            <!-- 部件血量加多少% -->
			<scaleAdjustment>0.75</scaleAdjustment>
		</li>
    </comps>
    <hediffGivers>
      <li Class="HediffGiver_Random">
        <mtbDays>31</mtbDays>
        <hediff>DMS_BionicShock</hediff>
