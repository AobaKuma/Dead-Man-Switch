  <!-- 馱馬 -->

  <!-- T1 DMS_DeadMansSwitch-->
  <!-- 獵犬 -->
  <ThingDef Name="DMS_MechHoundBase" ParentName="BaseDMSMachine" Abstract="True">  <!-- DMS_Mech_Hound 獵犬常規型 -->
    <statBases>
      <MoveSpeed>5</MoveSpeed>
      <ArmorRating_Blunt>0.2</ArmorRating_Blunt>
      <ArmorRating_Sharp>0.18</ArmorRating_Sharp>
      <!--<BandwidthCost MayRequire="Ludeon.Rimworld.Biotech">1</BandwidthCost>
      <WastepacksPerRecharge MayRequire="Ludeon.Rimworld.Biotech">2</WastepacksPerRecharge>-->
    </statBases>
    <race>
      <body>DMS_Hound</body>
      <baseBodySize>0.65</baseBodySize>
      <baseHealthScale>0.8</baseHealthScale>
      <!--<mechWeightClass>Light</mechWeightClass>-->
      <thinkTreeMain>VFE_Mechanoids_ViolentMachine</thinkTreeMain>
    </race>
    <comps>
			<li Class="VFE.Mechanoids.CompProperties_Machine">
        <violent>true</violent>
				<hoursActive>36</hoursActive>
			</li>
		</comps>
  </ThingDef>

  <ThingDef ParentName="DMS_MechHoundBase">  <!-- DMS_Mech_Hound_RC 獵犬常規型 -->
    <defName>DMS_Mech_Hound</defName>
    <label>hound</label>
    <description>A four-legged robot dog, can provide vanguard Scouting and early warning detect during armed operations.</description>
  </ThingDef>
  <PawnKindDef ParentName="BaseAutomatroidKind">
    <defName>DMS_Mech_Hound</defName>
    <label>hound</label>
    <race>DMS_Mech_Hound</race>
    <isFighter>true</isFighter>
    <combatPower>30</combatPower>
    <maxPerGroup>15</maxPerGroup>
    <lifeStages>
      <li>
        <bodyGraphicData>
          <texPath>Things/Automatroid/hound</texPath>
          <shaderType>CutoutComplex</shaderType>
          <graphicClass>Graphic_Multi</graphicClass>
          <drawSize>1.1</drawSize>
        </bodyGraphicData>
      </li>
      <li>
        <bodyGraphicData>
          <texPath>Things/Automatroid/OldTexture/hound</texPath>
          <shaderType>CutoutComplex</shaderType>
          <graphicClass>Graphic_Multi</graphicClass>
          <drawSize>1.1</drawSize>
        </bodyGraphicData>
      </li>
    </lifeStages>
    <weaponTags></weaponTags>
  </PawnKindDef>

  <ThingDef ParentName="DMS_MechHoundBase">  <!-- DMS_Mech_Hound_RC 獵犬鎮暴型 -->
    <defName>DMS_Mech_Hound_RC</defName>
    <label>watchdog</label>
    <description>a sub-variant of hound, designed for riot control and prison patrol.</description>
    <comps>
		  <li Class="DMS.CompProperties_VehicleWeapon">
			  <defaultWeapon>DMS_Revolver</defaultWeapon>			  
        <drawSize>1</drawSize>
			  <drawOffset>(0,0,0)</drawOffset>
			  <turretRotationFollowPawn>true</turretRotationFollowPawn>			  
        <rotationSmoothTime>0.12</rotationSmoothTime>
      </li>
    </comps>
  </ThingDef>
  <PawnKindDef ParentName="BaseAutomatroidKind">
    <defName>DMS_Mech_Hound_RC</defName>
    <label>watchdog</label>
    <race>DMS_Mech_Hound_RC</race>
    <isFighter>true</isFighter>
    <combatPower>30</combatPower>
    <maxPerGroup>15</maxPerGroup>
    <lifeStages>
      <li>
        <bodyGraphicData>
          <texPath>Things/Automatroid/hound</texPath>
          <shaderType>CutoutComplex</shaderType>
          <graphicClass>Graphic_Multi</graphicClass>
          <drawSize>1.1</drawSize>
        </bodyGraphicData>
      </li>
      <li>
        <bodyGraphicData>
          <texPath>Things/Automatroid/OldTexture/hound</texPath>
          <shaderType>CutoutComplex</shaderType>
          <graphicClass>Graphic_Multi</graphicClass>
          <drawSize>1.1</drawSize>
        </bodyGraphicData>
      </li>
    </lifeStages>
    <weaponTags></weaponTags>
  </PawnKindDef>

  <ThingDef ParentName="DMS_MechHoundBase"><!-- DMS_Mech_Hound_SMG 獵犬壓制型 -->
    <defName>DMS_Mech_Hound_FS</defName>
    <label>hetzen</label>
    <description>a sub-variant of hound, designed for urban combat fire support.</description>
    <comps>
		  <li Class="DMS.CompProperties_VehicleWeapon">
			  <defaultWeapon>DMS_SubMachineGunMounted</defaultWeapon>			  
        <drawSize>1</drawSize>
			  <drawOffset>(0,0,0)</drawOffset>
			  <turretRotationFollowPawn>true</turretRotationFollowPawn>			  
        <rotationSmoothTime>0.12</rotationSmoothTime>
      </li>
    </comps>
  </ThingDef>
