  <EffecterDef>
    <defName>DMS_Effect_Printer_North</defName>
    <positionRadius>0.1</positionRadius>
    <maintainTicks>120</maintainTicks>
    <children>
      <li>
        <subEffecterClass>SubEffecter_SprayerContinuous</subEffecterClass>
        <fleckDef>Smoke</fleckDef>
        <burstCount>2~5</burstCount>
        <spawnLocType>OnSource</spawnLocType>
        <rotationRate>-20~20</rotationRate>
        <scale>0.25~1</scale>
        <angle>30~70</angle>
        <speed>0.09~0.15</speed>
        <positionOffset>(0,0,2.25)</positionOffset>
        <airTime>1.5</airTime>
      </li>
      <li>
        <subEffecterClass>SubEffecter_SprayerChance</subEffecterClass>
        <moteDef>Mote_SparkThrown</moteDef>
        <positionLerpFactor>0.49</positionLerpFactor>
        <chancePerTick>0.2</chancePerTick>
        <scale>0.24~0.34</scale>
        <airTime>0.05~0.1</airTime>
        <rotationRate>-240~240</rotationRate>
        <speed>7.2~16</speed>
        <angle>135~225</angle>
        <positionRadius>0.02</positionRadius>
        <spawnLocType>OnTarget</spawnLocType>
      </li>
    </children>
  </EffecterDef>

