<?xml version="1.0" encoding="utf-8"?>
<Defs>

  <PawnKindDef ParentName="BaseMechanoidKind">
    <defName>DMS_SynthroidKind_Male</defName>
    <label>Synthroid</label>
    <race>DMS_Synthroid_Male</race>
    <combatPower>150</combatPower>
    <apparelAllowHeadgearChance>0</apparelAllowHeadgearChance>
    <apparelMoney>350~600</apparelMoney>
    <aiAvoidCover>false</aiAvoidCover>
    <canStrip>true</canStrip>
    <allowInMechClusters>false</allowInMechClusters>
    <lifeStages>
      <li>
        <bodyGraphicData>
          <texPath>Things/Synthroid/Body/Synthroid_Male</texPath>
          <graphicClass>Graphic_Multi</graphicClass>
          <drawSize>1.5</drawSize>
          <shadowData>
            <volume>(0.6, 0.8, 0.6)</volume>
          </shadowData>
        </bodyGraphicData>
      </li>
      <li>
        <bodyGraphicData>
          <texPath>Things/Synthroid/Body/Synthroid_Male</texPath>
          <graphicClass>Graphic_Multi</graphicClass>
          <drawSize>1.5</drawSize>
          <shadowData>
            <volume>(0.6, 0.8, 0.6)</volume>
          </shadowData>
        </bodyGraphicData>
      </li>
    </lifeStages>
    <weaponMoney>9999~9999</weaponMoney>
    <weaponTags>
      <li>DMS_LMG</li>
      <li>AssaultRifle</li>
    </weaponTags>
    <techHediffsChance>1</techHediffsChance>
    <techHediffsMoney>9999~9999</techHediffsMoney>
    <controlGroupPortraitZoom>1.2</controlGroupPortraitZoom>
  </PawnKindDef>

